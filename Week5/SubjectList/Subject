package Week5.SubjectList;

public class Subject {
	private String name;
	private String id;
	public Subject() {
		super();
	}
	public Subject(String name, String id) {
		super();
		this.name = name;
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String toString(){
		String info="";
		info = "课程代码:"+this.id+"\t"+"课程名称:"+this.name;
		return info;
	}

}
